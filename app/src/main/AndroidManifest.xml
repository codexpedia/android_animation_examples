<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sample.animations">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sample.animations.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.sample.animations.SlideActivity"/>
        <activity android:name="com.sample.animations.RotationActivity"/>
        <activity android:name=".ScaleActivity"/>
        <activity android:name=".ScaleRotationActivity"/>
        <activity android:name=".InterpolatorActivity"/>
    </application>

</manifest>
